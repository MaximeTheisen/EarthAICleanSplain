{
    "name": "claros",
    "description": "Search for anything on a website!",
    "version": "0.0.2",
    "manifest_version": 3,
    "icons": {
        "160": "img/icon.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "img/icon.png"
    },
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "worker.js"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "mac": "Command+K",
                "windows": "Ctrl+K",
                "linux": "Ctrl+K"
            },
            "description": "Start a new search"
        }
    }
}
